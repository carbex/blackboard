<!DOCTYPE html>
<html lang="en">

  <%- include('header')%>

  <body id="page-top">

    <%- include('navbar') %>

    <div id="wrapper">

      <!-- Sidebar -->
      <ul class="sidebar navbar-nav">
        <li class="nav-item">
          <a class="nav-link" href="/">
            <i class="fas fa-fw fa-tachometer-alt"></i>
            <span>Dashboard</span>
          </a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/users-page">
            <i class="fas fa-fw fa-users"></i>
            <span>Clients</span></a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/catalog-page">
            <i class="fas fa-fw fa-table"></i>
            <span>Catalogue Produits</span></a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/orders-list-page">
            <i class="fas fa-shopping-basket"></i>
            <span>Commandes</span></a>
        </li>

        <li class="nav-item">
          <a class="nav-link" href="/charts">
            <i class="fas fa-fw fa-chart-area"></i>
            <span>Charts</span></a>
        </li>
      </ul>

      <div id="content-wrapper">

        <div class="container-fluid">



          <div class="row">

            <div class="col-12 col-md-4 mb-3 chart">
              <canvas id="myChart1" width="100" height="100" data-countmale="<%= countMale %>" data-countfemale="<%= countFemale %>"></canvas> 
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <canvas id="myChart2" width="100" height="100"data-unreadmessages="<%= unreadMessages %>" data-readmessages="<%= readMessages %>"></canvas> 
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <canvas id="myChart3" width="100" height="100"data-paidandunshipted="<%= paidAndUnshipted %>" data-paidandshipted="<%= paidAndShipted %>"></canvas> 
            </div>

            <div class="col-12 col-md-4 mb-3 chart">
              <canvas id="myChart4" width="100" height="100"data-turnoverstringyfied="<%= turnoverStringyfied %>"></canvas> 
            </div>
            
          </div>

        </div>
        <!-- /.container-fluid -->

        <!-- Sticky Footer -->
        <footer class="sticky-footer">
          <div class="container my-auto">
            <div class="copyright text-center my-auto">
              <span>Copyright Â© Blackboard</span>
            </div>
          </div>
        </footer>

      </div>
      <!-- /.content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Chart script -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="./js/dataviz.js"></script>

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Page level plugin JavaScript-->
    <script src="vendor/chart.js/Chart.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin.min.js"></script>

  </body>

</html>
